{
  "expo": {
    "name": "native-uber",
    "slug": "native-uber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/background2.png",
      "resizeMode": "contain",
      "backgroundColor": "#C0B7A1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#C0B7A1"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router", {"origin": "https://uber.com/"}]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}